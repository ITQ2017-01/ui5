<?xml version="1.0" encoding="utf-8"?>
<mvc:View
   controllerName="hts.itq2017.walkthrough.controller.App"
   xmlns="sap.m"
   xmlns:mvc="sap.ui.core.mvc"
   xmlns:core="sap.ui.core">
   <VBox>
      <Text text="Rainer Text"/>
      <core:HTML content="&lt;hr&gt;"/>
      <Link text="Google Link" href="https://google.de/"/>
      <core:HTML content="&lt;hr&gt;"/>
      <Image src="https://www.animierte-gifs.net/data/media/107/animiertes-tanzen-bild-0243.gif"/>
      <core:HTML content="&lt;hr&gt;"/>
      <Button text="{i18n>showHelloButtonText}" press="onHello"/>
      <Button text="???" press="onAlert"/>
      <core:HTML content="&lt;hr&gt;"/>
      <Input value="{/recipient/given_name}" description="Hallo {/recipient/given_name} {/recipient/family_name}!" valueLiveUpdate="true" width="60%"/>
   </VBox>
</mvc:View>